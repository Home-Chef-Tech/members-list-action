{
  "name": "list-members-action",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "version": "1.0.0",
  "description": "GitHub Action to generate an organization's members list as JSON",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "jshint --exclude='node_modules/' *.js",
    "build": "parcel build  --no-source-maps --target=node --no-optimize index.js"
  },
  "targets": {
    "node": {
      "includeNodeModules": true
    }
  },
  "engines": {
    "node": "20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/homechef/members-list-action.git"
  },
  "keywords": [
    "github",
    "action"
  ],
  "license": "GPL-3.0",
  "homepage": "https://github.com/homechef/members-list-action#readme",
  "dependencies": {
    "@actions/core": "^1.10",
    "@actions/github": "^6.0.0",
    "jshint": "^2.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "parcel": "^2.10.2"
  }
}
